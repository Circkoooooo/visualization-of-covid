{
	"env": {
		"browser": true,
		"es2021": true
	},
	"extends": ["plugin:vue/essential", "airbnb-base"],
	"parserOptions": {
		"ecmaVersion": "latest",
		"parser": "@typescript-eslint/parser",
		"sourceType": "module"
	},
	"plugins": ["vue", "@typescript-eslint"],
	"rules": {
		"linebreak-style": "off",
		"no-tabs": "off",
		"no-undef": "off",
		"import/extensions": "off",
		"import/no-unresolved": "off"
	}
}
